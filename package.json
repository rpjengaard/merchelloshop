	"scripts": {
		"start": "cross-env NODE_ENV=production node --inspect server/index",
		"dev": "node --inspect server/index",
		"dist": "yarn install && rimraf dist && yarn run build:client && yarn run build:server",
		"build": "yarn run dist && yarn run devWeb:copy && yarn run devWeb:install",
		"build:client": "cross-env NODE_ENV=production webpack --config frontend/build/webpack.client.config.js --hide-modules --display 'minimal'",
		"build:server": "cross-env NODE_ENV=production webpack --config frontend/build/webpack.server.config.js --hide-modules --display 'minimal'",
		"devWeb:copy": "rimraf Dev/web/node/dist && cpy package.json Dev/web/node && cpy SkyLogError.js Dev/web/node && cpy yarn.lock Dev/web/node && cpy server/**.* Dev/web/node --parents && cpy dist/**/*.* Dev/web/node --parents",
		"devWeb:install": "cd Dev/web/node && yarn install --production"
	},
	"engines": {
		"node": ">=6.1",
		"npm": ">=4.0"
	},
